<?php
$pwd = "4321";
$hash = password_hash($pwd, PASSWORD_DEFAULT);
/* $2y: Indica che si usa l'algoritmo bcript
   $10: Indica il numero di iterazioni fatte, in questo caso 2 alla 10
*/
echo $hash;
echo '<br>';
echo strlen($hash);
echo '<br>';
echo(password_verify($pwd,$hash) ? 'giusta' : 'sbagliata');
